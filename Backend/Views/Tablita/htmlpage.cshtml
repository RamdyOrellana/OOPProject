@model List<Registro>

<table class="table">
    <thead>
        <tr>
            <th>Nombres</th>
            <th>Apellidos</th>
            <th>TipoDocumento</th>
     
        </tr>
    </thead>
    <tbody>
        @foreach (var registro in Model)
        {
            <tr>
                <td>@registro.Nombres</td>
                <td>@registro.Apellidos</td>
                <td>@registro.TipoDocumento</td>
              
            </tr>
        }
    </tbody>
</table>

<div class="text-center">
    <nav aria-label="Paginación">
        <ul class="pagination">
            @for (int i = 1; i <= ViewBag.TotalPaginas; i++)
            {
                <li class="page-item @(i == ViewBag.PaginaActual ? "active" : "")">
                    <a class="page-link" href="/Tablita/Vista?pagina=@i">@i</a>
                </li>
            }
        </ul>
    </nav>
</div>